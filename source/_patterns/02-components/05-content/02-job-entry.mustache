<section>
  {{#job_entry}}
    <article class="max-mb-double">
      <h5 class="max-color-primary-darker max-mb-none">
        {{title}}
      </h5>

      <tiny class="max-font-weight-light max-italic max-color-primary-dark">
        <span class="max-font-weight-semibold">{{company}}</span> from {{start}} to {{end}}
      </tiny>

      <div class="max-pl-half max-pl-base-sm max-pl-double-md">
        <p>
          <strong>Skills Used:</strong> {{skills}}
        </p>

        <p>
          {{descr}}
        </p>
      </div>
    </article>
  {{/job_entry}}
</section>